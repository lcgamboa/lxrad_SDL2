CC = g++
FLAGS = -Wall -ggdb -I../../include/  -D_REENTRANT -fpermissive

OBJS = pimageview.o imageview1.o  


all: $(OBJS)
	$(CC) $(FLAGS) $(OBJS) -oimageview ../../lib/liblxrad.a -lX11 -lXpm -lImlib2

%.o: %.cc
	$(CC) -c $(FLAGS) $< 

run: all
	./imageview

install:

clean:
	rm -f imageview *.o core
